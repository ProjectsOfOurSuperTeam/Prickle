<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <!--To inherit the global NuGet package sources remove the <clear/> line below -->
        <clear />
        <add key="nuget" value="https://api.nuget.org/v3/index.json" />
        <add key="davidfowl-aspire" value="https://f.feedz.io/davidfowl/aspire/nuget/index.json" allowInsecureConnections="True" />
    </packageSources>

    <packageSourceMapping>
        <!-- key value for <packageSource> should match key values from <packageSources> element -->
        <clear />
        <packageSource key="nuget">
            <package pattern="*" />
        </packageSource>
		<packageSource key="davidfowl-aspire">
			<package pattern="Aspire.Hosting.Docker.SshDeploy" />
		</packageSource>
    </packageSourceMapping>
</configuration>